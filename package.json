{
  "name": "dashboard-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://Ferigit.github.io/dashboard-app",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.9.0",
    "lucide-react": "^0.513.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-router-dom": "^7.6.1",
    "tailwindcss": "^4.1.8",
    "zod": "^3.25.42",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "^9.0.3",
    "@storybook/addon-docs": "^9.0.3",
    "@storybook/addon-onboarding": "^9.0.3",
    "@storybook/addon-vitest": "^9.0.3",
    "@storybook/react-vite": "^9.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.3",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "happy-dom": "^17.5.6",
    "jsdom": "^26.1.0",
    "msw": "^2.8.7",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "storybook": "^9.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-gh-pages": "^1.0.1",
    "vitest": "^3.1.4"
  }
}
